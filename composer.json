{
    "name": "yormy/tripwire-laravel",
    "description": "",
    "keywords": [
        "laravel",
        "tripwire",
        "firewall",
        "security",
        "waf",
        "blacklist",
        "xss",
        "sqli",
        "rfi",
        "lfi",
        "web application firewall"
    ],
    "homepage": "https://yormy.github.io/tripwire-laravel/",
    "license": "MIT",
    "authors": [
        {
            "name": "Yormy",
            "email": "dev@yormy.com",
            "homepage": "https://yormy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/container": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "laravel/framework": "^9",
        "orchestra/testbench": "^7",
        "jenssegers/agent": "^2.6",
        "yormy/xid": "dev-master",
        "rennokki/laravel-eloquent-query-cache": "^3.3.1",
        "nesbot/carbon": "^2.67.0"
    },
    "require-dev": {
        "jaschilz/php-coverage-badger": "^2.0",
        "phpmd/phpmd": "^2.12",
        "phpstan/phpstan": "^1.10.19",
        "phpunit/phpunit": "^9.6.9",
        "psalm/plugin-laravel": "^2.8",
        "squizlabs/php_codesniffer": "^3.7",
        "thibautselingue/local-php-security-checker-installer": "^1",
        "vimeo/psalm": "^5.12",
        "laravel/pint": "^1.10.2",
        "spatie/laravel-ray": "^1.32",
        "brianium/paratest": "^6.10"
    },
    "autoload": {
        "psr-4": {
            "Yormy\\TripwireLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yormy\\TripwireLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs-fixer" : "phpcbf",
        "md": "vendor/bin/phpmd . html cleancode --reportfile report.html",
        "phpcs": "phpcs --standard=PSR2 -s --exclude=Generic.Files.LineLength --standard=./phpcs.xml --report-file=phpcs-report.txt",
        "phpstan": "vendor/bin/phpstan analyze src --error-format=raw --level=4",
        "psalm": "vendor/bin/psalm --no-diff --no-cache --report-show-info=false --report=psalm-report.txt",
        "psalm-debug": "vendor/bin/psalm --no-cache --debug --debug-by-line",
        "pint": "vendor/bin/pint",
        "test": "vendor/bin/phpunit --colors=always --testdox --testsuite Main",
        "testp": "vendor/bin/paratest --colors=always --testdox --testsuite Main",
        "test-ext": "vendor/bin/phpunit --colors=always --testdox --testsuite Extensive",
        "test-extp": "vendor/bin/paratest --colors=always --testdox --testsuite Extensive",
        "test-browser": "vendor/bin/phpunit tests/Browser --colors=always --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yormy\\TripwireLaravel\\TripwireServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
